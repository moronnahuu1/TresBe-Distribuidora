{
    "version": 2,
    "builds": [
      {
        "src": "src/app/**/*",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/angular-app" }
      },
      {
        "src": "api/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  